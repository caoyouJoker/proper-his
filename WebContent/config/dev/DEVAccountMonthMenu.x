 # # Title: 设备类别参数设定设定 # # Description:设备类别参数设定设定 # # Copyright: JavaHis (c) 2008 # # @author sundx # @version 1.0<Type=TMenuBar>UI.Item=File;WindowUI.button=print;dep;month;clear;closeWindow.Type=TMenuWindow.Text=窗口Window.M=WWindow.Item=RefreshFile.Type=TMenuFile.Text=文件File.M=F File.Item=print;dep;month;clear;closeprint.Type=TMenuItemprint.Text=打印print.Tip=打印print.M=Pprint.key=Ctrl+Pprint.Action=onPrintprint.pic=print.gif dep.Type=TMenuItemdep.Text=折旧计算dep.Tip=折旧计算 dep.M=Qdep.key=Ctrl+Udep.Action=onDepdep.pic=Undo.gif month.Type=TMenuItemmonth.Text=月结结转month.Tip=月结结转 month.M=Qmonth.key=Ctrl+Fmonth.Action=onMonthmonth.pic=save.gif clear.Type=TMenuItemclear.Text=清空clear.Tip=清空(Ctrl+Z)clear.M=Cclear.key=Ctrl+Zclear.Action=onClearclear.pic=clear.gifclose.Type=TMenuItemclose.Text=退出close.Tip=退出close.M=Xclose.key=Alt+F4close.Action=onCloseclose.pic=close.gif Refresh.Type=TMenuItem Refresh.Text=刷新Refresh.Tip=刷新Refresh.M=RRefresh.key=F5Refresh.Action=onResetRefresh.pic=Refresh.gif